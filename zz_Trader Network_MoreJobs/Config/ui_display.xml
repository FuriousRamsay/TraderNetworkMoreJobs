		<item_display_info display_type="FuriousRamsayPet" display_group="groupPets">
		</item_display_info>

		<item_display_info display_type="FuriousRamsayAlly" display_group="groupAllies">
		</item_display_info>

		<item_display_info display_type="FuriousRamsayEmpty">
		</item_display_info>

		<item_display_info display_type="FuriousRamsayBanditClothingAviatorGoggles" display_group="groupAttire">
